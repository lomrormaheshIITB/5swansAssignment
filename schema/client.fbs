namespace MyNamespace;

enum Gender: byte {
    Male: 0,
    Female: 1
}

table Person {
    name: string;
    age: ushort;
    weight: float;
    gender: Gender;
}

table Group {
    groupName: string;
    age: float;
    weight: float;
    members: [string];
}

union Client {
    Person,
    Group
}


root_type Client;